{
  "name": "pseudoalgo",
  "displayName": "pseudoAlgo",
  "publisher": "RhaymTech",
  "description": "Pseudocode utilisé au des universités algeriennes au module d'ASD (Algoritmique et Structure des donnés)",
  "version": "2.0.0",
  "icon": "./icon.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "algo",
      "aliases": ["Algorithmique", "algo"],
      "extensions": [".algo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "algo",
      "scopeName": "source.algo",
      "path": "./syntaxes/algo.tmLanguage.json"
    }],
    "snippets": [{
      "language": "algo",
      "path": "./snippets/snippets.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhaym-tech/pseudoalgo.git"
  }
}
